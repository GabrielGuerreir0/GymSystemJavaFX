<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="549.0" prefWidth="526.0" spacing="10" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.CadastroPersonalController">
    <padding>
        <Insets bottom="20" left="20" right="20" top="20" />
    </padding>
   <HBox prefHeight="69.0" prefWidth="486.0">
      <children>
      
                <!-- Campo Nome -->
          <Label text="Nome:">
            <HBox.margin>
               <Insets left="100.0" />
            </HBox.margin>
         </Label>
          <TextField fx:id="nomeField" style="-fx-background-color: #989494;">
            <HBox.margin>
               <Insets left="150.0" right="50.0" />
            </HBox.margin>
         </TextField>
      </children>
   </HBox>
   <HBox prefHeight="64.0" prefWidth="486.0">
      <children>
      
                                  <!-- Campo CPF -->
          <Label text="CPF:">
            <HBox.margin>
               <Insets left="100.0" />
            </HBox.margin>
         </Label>
          <TextField fx:id="cpfField" style="-fx-background-color: #989494;">
            <HBox.margin>
               <Insets left="163.0" />
            </HBox.margin>
         </TextField>
      </children>
   </HBox>
   <HBox prefHeight="63.0" prefWidth="486.0">
      <children>
      
          <!-- Campo Telefone -->
          <Label text="Telefone:">
            <HBox.margin>
               <Insets left="100.0" />
            </HBox.margin>
         </Label>
          <TextField fx:id="telefoneField" style="-fx-background-color: #989494;">
